{
  "name": "whoiz",
  "version": "0.0.1",
  "main": "dist/index",
  "types": "dist/index",
  "bin": {
    "whoiz": "./dist/index.js"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint && npm run format && npm run build",
    "version": "git add -A",
    "postversion": "git push && git push --tags",
    "test": "jest",
    "format": "prettier --write \"src/**/*.{js,ts}\"",
    "lint": "prettier --check \"src/**/*.{js,ts}\""
  },
  "author": {
    "name": "Nick Porter",
    "email": "github.dom@surj.email"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "prettier --check"
    ]
  },
  "devDependencies": {
    "@types/dedent": "^0.7.0",
    "@types/jest": "^26.0.10",
    "husky": "^4.2.5",
    "jest": "^26.4.1",
    "lint-staged": "^10.2.11",
    "prettier": "^2.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "date-fns": "^2.15.0",
    "dedent": "^0.7.0",
    "whois": "^2.13.0"
  }
}
